<% case be
   when "opencl"
     event = "event"
     prof_end = "end"
     mulfactor = 1
     profiled_call = "clEnqueueSVMFree"
     backend = 2
   when "ze"
     event = "hEvent"
     prof_end = "globalEnd"
     mulfactor = 1
     profiled_call = "zeCommandListAppendBarrier"
     backend = 1
   when "cuda"
     event = "hStop"
     prof_end = "milliseconds"
     mulfactor = 1000000
     profiled_call = "cuLaunchHostFunc"
     backend = 3
   end %>

in:
  :hostname: aurora12.gov
  :default_stream: stream0
  :stream_classes:
  - :name: stream_class
    :schemas:
    - thapi_<%= be %>
    :clock_snapshot_value: true
    :common_context:
    - :name: vpid
      :class: signed
    - :name: vtid
      :class: unsigned
  :streams:
  - :name: stream0
    :class: stream_class
  :events:
  <% if be == "cuda" %>
  - :name: lttng_ust_cuda:cuDevicePrimaryCtxRetain_entry
    :common_context:
      vtid: 1
  - :name: lttng_ust_cuda:cuDevicePrimaryCtxRetain_exit
    :common_context:
      vtid: 1
  - :name: lttng_ust_cuda:cuCtxSetCurrent_entry
    :payload:
      ctx: 1
    :common_context:
      vtid: 1
  - :name: lttng_ust_cuda:cuCtxSetCurrent_exit
    :common_context:
      vtid: 1
  - :name: lttng_ust_cuda:cuCtxCreate_entry
    :common_context:
      vtid: 2
  - :name: lttng_ust_cuda:cuCtxCreate_exit
    :common_context:
      vtid: 2
  <% end %>
  - :name: lttng_ust_<%= be %>:<%= profiled_call %>_entry
    :clock_snapshot_value: 0
    :common_context:
      vtid: 1
  - :name: lttng_ust_<%= be %>:<%= profiled_call %>_entry
    :clock_snapshot_value: 0
    :common_context:
      vtid: 2
  - :name: lttng_ust_<%= be %>_profiling:event_profiling
    :common_context:
      vtid: 1
    :payload:
      <%= event %>: 11
  - :name: lttng_ust_<%= be %>_profiling:event_profiling
    :common_context:
      vtid: 2
    :payload:
      <%= event %>: 12
  - :name: lttng_ust_<%= be %>:<%= profiled_call %>_exit
    :common_context:
      vtid: 2
    :clock_snapshot_value: 3
  - :name: lttng_ust_<%= be %>:<%= profiled_call %>_exit
    :common_context:
      vtid: 1
    :clock_snapshot_value: 3
  - :name: lttng_ust_<%= be %>_profiling:event_profiling_results
    :common_context:
      vtid: 2
    :payload:
      <%= event %>: 12
      <%= prof_end %>: 10
  - :name: lttng_ust_<%= be %>_profiling:event_profiling_results
    :common_context:
      vtid: 1
    :payload:
      <%= event %>: 11
      <%= prof_end %>: 10
ref:
  :default_stream: stream0
  :stream_classes:
  - :name: interval_class
    :schemas:
    - thapi_interval
    :common_context:
    - :name: hostname
      :class: string
    - :name: vpid
      :class: signed
    - :name: vtid
      :class: unsigned
    - :name: ts
      :class: unsigned
    - :name: backend
      :class: signed
  :streams:
  - :name: stream0
    :class: interval_class
  :events:
  <% if be == "cuda" %>
  - :name: lttng:host
    :common_context:
      vtid: 1
      hostname: aurora12.gov
      backend: <%= backend %>
    :payload:
      name: cuDevicePrimaryCtxRetain
      dur: 1
  - :name: lttng:host
    :common_context:
      vtid: 1
      hostname: aurora12.gov
      backend: <%= backend %>
      ts: 2
    :payload:
      name: cuCtxSetCurrent
      dur: 1
  - :name: lttng:host
    :common_context:
      vtid: 2
      hostname: aurora12.gov
      backend: <%= backend %>
      ts: 4
    :payload:
      name: cuCtxCreate
      dur: 1
  <% end %>
  - :name: lttng:host
    :common_context:
      vtid: 2
      hostname: aurora12.gov
      backend: <%= backend %>
    :payload:
      name: <%= profiled_call %>
      dur: 3

  - :name: lttng:host
    :common_context:
      vtid: 1
      hostname: aurora12.gov
      backend: <%= backend %>
    :payload:
      name: <%= profiled_call %>
      dur: 3

  - :name: lttng:device
    :common_context:
      vtid: 2
      hostname: aurora12.gov
      backend: <%= backend %>
    :payload:
      name: <%= profiled_call %>
      dur: <%= 10 * mulfactor %>

  - :name: lttng:device
    :common_context:
      vtid: 1
      hostname: aurora12.gov
      backend: <%= backend %>
    :payload:
      name: <%= profiled_call %>
      dur: <%= 10 * mulfactor %>
