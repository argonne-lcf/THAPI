<% event = "hStop"
   prof_end = "milliseconds"
   mulfactor = 1000000
   profiled_call = "cuLaunchHostFunc"
   backend = 3
   context1 = 7
   stream1 = 7
   dev1 = 7
   context2 = 100
   dev2 = 100
%>

in:
  :hostname: aurora12.gov
  :default_stream: stream0
  :stream_classes:
  - :name: stream_class
    :schemas:
    - thapi_<%= be %>
    :clock_snapshot_value: true
    :common_context:
    - :name: vpid
      :class: signed
    - :name: vtid
      :class: unsigned
  :streams:
  - :name: stream0
    :class: stream_class
  :events:
  - :name: lttng_ust_cuda:cuCtxCreate_entry
    :payload:
      dev: <%= dev1 %>
  - :name: lttng_ust_cuda:cuCtxCreate_exit
    :payload:
      pctx_val: <%= context1 %>
  - :name: lttng_ust_cuda:cuStreamCreate_entry
  - :name: lttng_ust_cuda:cuStreamCreate_exit
    :payload:
      phStream_val: <%= stream1 %>
  - :name: lttng_ust_cuda:cuCtxCreate_entry
    :payload:
      dev: <%= dev2 %>
  - :name: lttng_ust_cuda:cuCtxCreate_exit
    :payload:
      pctx_val: <%= context2 %>
  - :name: lttng_ust_<%= be %>:<%= profiled_call %>_entry
    :clock_snapshot_value: 0
    :payload:
      hStream: <%= stream1 %>
  - :name: lttng_ust_<%= be %>_profiling:event_profiling
    :payload:
      <%= event %>: 12
  - :name: lttng_ust_<%= be %>:<%= profiled_call %>_exit
    :clock_snapshot_value: 3
  - :name: lttng_ust_<%= be %>_profiling:event_profiling_results
    :payload:
      <%= event %>: 12
      <%= prof_end %>: 10
ref:
  :default_stream: stream0
  :stream_classes:
  - :name: interval_class
    :schemas:
    - thapi_interval
    :common_context:
    - :name: hostname
      :class: string
    - :name: vpid
      :class: signed
    - :name: vtid
      :class: unsigned
    - :name: ts
      :class: unsigned
    - :name: backend
      :class: signed
  :streams:
  - :name: stream0
    :class: interval_class
  :events:
  - :name: lttng:host
    :common_context:
      hostname: aurora12.gov
      backend: <%= backend %>
    :payload:
      name: cuCtxCreate
      dur: 1
  - :name: lttng:host
    :common_context:
      hostname: aurora12.gov
      backend: <%= backend %>
      ts: 2
    :payload:
      name: cuStreamCreate
      dur: 1
  - :name: lttng:host
    :common_context:
      hostname: aurora12.gov
      backend: <%= backend %>
      ts: 4
    :payload:
      name: cuCtxCreate
      dur: 1
  - :name: lttng:host
    :common_context:
      hostname: aurora12.gov
      backend: <%= backend %>
    :payload:
      name: <%= profiled_call %>
      dur: 3
  - :name: lttng:device
    :common_context:
      hostname: aurora12.gov
      backend: <%= backend %>
    :payload:
      name: <%= profiled_call %>
      dur: <%= 10 * mulfactor %>
      did: <%= dev1 %>
      sdid: <%= dev1 %>
